{
  "hosting": {
    "public": "dist/frontend/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/**",
        "destination": "https://yojanaai.in",
        "type": 301,
        "glob": "**",
        "locale": "en",
        "hostname": "yojanaai.web.app"
      },
      {
        "source": "/**",
        "destination": "https://yojanaai.in",
        "type": 301,
        "glob": "**",
        "locale": "en",
        "hostname": "yojanaai.firebaseapp.com"
      }
    ]
  }
}